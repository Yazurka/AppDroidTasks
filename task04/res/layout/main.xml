<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
<fragment
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:name="org.guut.fragments.fragments.MyListFragment"
    tools:layout="@layout/mylistfragment"></fragment>
    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="org.guut.fragments.fragments.pictureFrame"
        tools:layout="@layout/mypictureframefragment"></fragment>
    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="org.guut.fragments.fragments.pictureText"
        tools:layout="@layout/mypicturetextfragment"></fragment>
</LinearLayout>