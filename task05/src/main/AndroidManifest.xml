<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="1"
    android:versionName="1.0"
    package="no.hist.itfag.books">
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="no.hist.itfag.books.BookActivity"
            android:label="@string/title_activity_book" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.hist.itfag.calc.CalcActivity" />
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
        <activity android:name="org.guut.task05.SettingsActivity">
            <intent-filter>
                <action android:name="org.guut.task05.SettingsActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>